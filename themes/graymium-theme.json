  "tokenColors": [
    {
      "scope": [
        "string",
        "string.other.link",
        "string.other.link.title.markdown",
        "constant.other.symbol",
        "entity.other.inherited-class",
        "markup.underline.link.image.markdown",
        "markup.raw.inline",
        "markup.inserted",
        "markup.inserted.git_gutter",
        "variable.parameter.url.scss"
      ],
      "settings": {
        "foreground": "#92D192"
      }
    },
    {
      "scope": [
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#649064"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.operator.expression.typeof",
        "keyword.operator.new",
        "keyword.operator.expression.import.js",
        "variable.language.this",
        "storage",
        "support.function.construct.output.php",
        "markup.inline.raw.string.markdown"
      ],
      "settings": {
        "foreground": "#FFEEA6"
      }
    },
    {
      "scope": [
        "entity.name.section",
        "entity.other.attribute-name.class.css",
        "markup.heading, punctuation.definition.heading",
        "string.unquoted.alias.graphql",
        "punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#FFD479"
      }
    },
    {
      "scope": [
        "meta.selector",
        "meta.function-call",
        "meta.property-value",
        "entity.name.function.ts",
        "entity.name.function.js",
        "entity.name.function.tsx",
        "entity.name.function.php",
        "variable.other.property.php",
        "variable.other.member.php"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": [
        "meta.tag",
        "entity.name.type.ts",
        "entity.name.function.graphql",
        "keyword.other.special-method",
        "markup.bold",
        "punctuation.definition.bold",
        "invalid.illegal.unrecognized-tag.html",
        "support.variable.dom.js",
        "support.variable.dom.ts"
      ],
      "settings": {
        "foreground": "#6AB0F3"
      }
    },
    {
      "scope": [
        "meta.type.annotation.ts",
        "support.type.primitive.ts",
        "entity.name.type.ts",
        "meta.type.annotation.ts",
        "variable.other.jsdoc"
      ],
      "settings": {
        "foreground": "#4A7BAA"
      }
    },
    {
      "scope": [
        "entity.tag.tagbraces.nunjucks",
        "punctuation.colon.graphql",
        "variable.parameter.ts",
        "variable.parameter.tsx",
        "string.unquoted.filter-pipe.nunjucks",
        "string.unquoted.graphql"
      ],
      "settings": {
        "foreground": "#848991"
      }
    },
    {
      "scope": [
        "comment",
        "meta.template.expression.js",
        "punctuation.separator",
        "punctuation.definition.comment",
        "punctuation.definition.template-expression.begin.js",
        "punctuation.definition.template-expression.end.js",
        "punctuation.definition.template-expression.begin.ts",
        "punctuation.definition.template-expression.end.ts",
        "punctuation.definition.metadata.markdown",
        "punctuation.support.type.property-name.begin.json",
        "punctuation.support.type.property-name.end.json",
        "markup.underline.link.markdown"
      ],
      "settings": {
        "foreground": "#7A8085"
      }
    },
    {
      "scope": [
        "punctuation.definition.variable",
        "variable",
        "variable.language.this.php",
        "markup.deleted",
        "markup.deleted.git_gutter",
        "invalid.illegal",
        "string.unquoted.tag-string.nunjucks"
      ],
      "settings": {
        "foreground": "#F2777A"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "keyword.other.unit",
        "markup.quote"
      ],
      "settings": {
        "foreground": "#FCA369"
      }
    },
    {
      "scope": [
        "constant",
        "entity.other.njk.delimiter.tag",
        "entity.other.njk.delimiter.variable",
        "entity.other.attribute-name.id",
        "punctuation.tag.njk",
        "punctuation.output.njk",
        "keyword.control.njk",
        "keyword.operator.njk",
        "keyword.control.flow.js",
        "support.variable.njk",
        "support.function",
        "support.constant.property-value",
        "markup.italic",
        "markup.changed",
        "markup.changed.git_gutter",
        "storage.type.function.js",
        "variable.parameter.njk",
        "variable.other.njk",
        "punctuation.definition.tag.begin.njk",
        "punctuation.definition.tag.end.njk",
        "meta.embedded.block.frontmatter.njk"
      ],
      "settings": {
        "foreground": "#E1A6F2"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "constant.character.escape",
        "constant.character.escape.json",
        "entity.other.attribute-name.pseudo-element.css",
        "entity.other.attribute-name.pseudo-class.css",
        "keyword.operator",
        "punctuation.accessor.js",
        "punctuation.definition.block.tag.jsdoc",
        "storage.type.class.jsdoc"
      ],
      "settings": {
        "foreground": "#AC8D58"
      }
    },
    {
      "scope": [
        "punctuation.definition.block",
        "meta.parameters",
        "punctuation.definition.parameters",
        "meta.brace.round",
        "punctuation.definition.dictionary",
        "constant.character.escape.backslash.regexp",
        "punctuation.section",
        "punctuation.terminator.statement",
        "variable.other.property.js",
        "support.variable.property.dom.js",
        "punctuation.definition.parameters",
        "punctuation.definition.array",
        "support.type.property-name.json",
        "entity.name.function",
        "meta.require",
        "support.function.any-method",
        "meta.tag.sgml.html",
        "punctuation.section.embedded",
        "variable.interpolation",
        "variable.object.property.ts",
        "variable.object.property.tsx",
        "meta.brace.square",
        "punctuation.definition.array",
        "punctuation.definition.binding-pattern.object.js",
        "entity.name.tag.yaml",
        "meta.selectionset.graphql"
      ],
      "settings": {
        "foreground": "#D2DADA"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.pseudo-class.css",
        "constant.other.php",
        "markup.italic.markdown",
        "punctuation.definition.italic.markdown",
        "string.regexp",
        "entity.other.attribute-name.pseudo-element.css",
        "meta.image.inline.markdown"
      ],
      "settings": {
        "foreground": "#62CFCF"
      }
    },
    {
      "scope": [
        "entity.name.tag.reference",
        "meta.embedded.block.php",
        "meta.embedded.line.php",
        "keyword.other.phpdoc.php",
        "meta.separator",
        "variable.object.property.ts",
        "variable.object.property.tsx",
        "variable.other.property.js",
        "support.variable.property.dom.js",
        "support.type.property-name.json",
        "entity.name.type.instance.jsdoc",
        "variable.graphql",
        "meta.template.expression.ts"
      ],
      "settings": {
        "foreground": "#ABB2BF"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796E6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#B267E6"
      }
    },
    {
      "scope": [
        "meta.object-literal.key.js",
        "support.class.njk",
        "variable.other.njk",
        "variable.parameter.njk",
        "punctuation.terminator",
        "meta.tag.metadata.doctype.html"
      ],
      "settings": {
        "foreground": "#B5BCC3"
      }
    }
