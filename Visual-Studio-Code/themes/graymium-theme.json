   "tokenColors": [
      {
         "scope": [
            "string",
            "string.other.link",
            "string.other.link.title.markdown",
            "constant.other.symbol",
            "entity.other.inherited-class",
            "markup.underline.link.image.markdown",
            "markup.raw.inline",
            "markup.inserted",
            "markup.inserted.git_gutter",
            "variable.parameter.url.scss"
         ],
         "settings": {
            "foreground": "#9FCF9F"
         }
      },
      {
         "scope": "punctuation.definition.string",
         "settings": {
            "foreground": "#649064"
         }
      },
      {
         "scope": [
            "storage",
            "keyword.operator.expression.typeof",
            "keyword.operator.expression.import.js",
            "markup.inline.raw.string.markdown",
            "entity.name.tag.variable.liquid",
            "variable.language.this.js"
         ],
         "settings": {
            "foreground": "#A99DEB"
         }
      },
      {
         "scope": [
            "entity.name.section",
            "markup.heading",
            "entity.other.attribute-name.class.css"
         ],
         "settings": {
            "foreground": "#E0BB70"
         }
      },
      {
         "scope": [
            "meta.tag",
            "entity.name.function.graphql",
            "entity.name.function.js"
         ],
         "settings": {
            "foreground": "#80B2E8"
         }
      },
      {
         "scope": [
            "comment",
            "punctuation.separator",
            "punctuation.definition.comment",
            "meta.template.expression.js",
            "punctuation.definition.template-expression.begin.js",
            "punctuation.definition.template-expression.end.js",
            "punctuation.support.type.property-name.begin.json",
            "punctuation.support.type.property-name.end.json",
            "punctuation.definition.metadata.markdown",
            "markup.underline.link.markdown"
         ],
         "settings": {
            "foreground": "#7A8085"
         }
      },
      {
         "scope": [
            "constant.numeric",
            "keyword.other.unit",
            "markup.quote"
         ],
         "settings": {
            "foreground": "#EB9F6C"
         }
      },
      {
         "scope": [
            "meta.separator",
            "meta.parameters",
            "keyword.operator",
            "entity.name.tag.reference",
            "support.variable.property.dom.js",
            "support.type.property-name.json",
            "variable.graphql",
            "variable.other.readwrite.js",
            "meta.object-literal.key.js",
            "keyword.other.definition",
            "entity.name.type.instance.jsdoc",
            "entity.name.tag.yaml",
            "meta.tag.metadata.doctype.html"
         ],
         "settings": {
            "foreground": "#ABB2BF"
         }
      },
      {
         "scope": [
            "punctuation.terminator",
            "punctuation.output.liquid",
            "meta.paragraph.markdown"
         ],
         "settings": {
            "foreground": "#C9CED6"
         }
      },
      {
         "scope": [
            "support.type.object.module.js"
         ],
         "settings": {
            "foreground": "#72C4CF"
         }
      },
      {
         "scope": [
            "entity.name.type.class.js"
         ],
         "settings": {
            "foreground": "#E0D59D"
         }
      },
      {
         "scope": [
            "invalid.illegal"
         ],
         "settings": {
            "foreground": "#E8808C"
         }
      },
      {
         "scope": [
            "keyword",
            "meta.embedded.block.liquid",
            "punctuation.definition.tag.begin.liquid",
            "punctuation.definition.tag.end.liquid",
            "keyword.other.dot.liquid",
            "keyword.operator.logical.liquid",
            "entity.name.tag.variable.liquid",
            "entity.name.tag.iteration.liquid",
            "entity.name.tag.conditional.liquid",
            "entity.name.tag.control.liquid",
            "entity.name.tag.method.liquid",
            "entity.name.tag.other.liquid",
            "keyword.operator.assignment.augmented.liquid",
            "keyword.operator.equality.liquid",
            "keyword.operator.expression.delete.js",
            "variable.language.this.js",
            "keyword.operator.logical.js",
            "keyword.operator.comparison.js",
            "keyword.operator.relational.js",
            "keyword.operator.new.js",
            "constant.language.undefined.js",
            "constant.language.boolean.true.js",
            "text.html.markdown",
            "constant.language.json.comments",
            "source.editorconfig",
            "support.function.filter.liquid"
         ],
         "settings": {
            "foreground": "#CC97DB"
         }
      },
      {
         "scope": "token.info-token",
         "settings": {
            "foreground": "#6796E6"
         }
      },
      {
         "scope": "token.warn-token",
         "settings": {
            "foreground": "#CD9731"
         }
      },
      {
         "scope": "token.error-token",
         "settings": {
            "foreground": "#F44747"
         }
      },
      {
         "scope": "token.debug-token",
         "settings": {
            "foreground": "#B267E6"
         }
      }
   ],
   "colorSpaceName": "sRGB"
}
